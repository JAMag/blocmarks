<%= form_for Bookmark.new do |f| %>
       <div class="form-group">
         <%= f.label :url %>
         <%= f.text_field :url, class: 'form-control', placeholder: "Enter  URL" %>
       </div>

      <div class="form-group">
         <%= f.label :topic %>
         <%= f.text_field :topic, class: 'form-control', placeholder: "Enter  topic" %>
       </div>
  
       <div class="form-group">
         <%= f.submit "Save", class: 'btn btn-success' %>
       </div>
<% end %>