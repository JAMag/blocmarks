<h1>All Topics</h1>
<% @topics.each do |topic| %>     
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <%= link_to topic.title, topic %>
      </h4>
      <ul>
        <% topic.bookmarks.each do |bookmark| %>
          <li><%= render partial: 'bookmarks/bookmark', locals: { bookmark: bookmark } %></li>
        <% end %>
      </ul>
    </div>
    <div class="media-body">
      <h4 class="media-heading">
      </h4>
    </div>
  </div>
<% end %>
